<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Owner_restricted</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISCHANGED(OwnerId),
  $User.Id &lt;&gt; &quot;00561000000df4U&quot;)</errorConditionFormula>
    <errorMessage>Please contact the System Administrator to change ownership.</errorMessage>
</ValidationRule>
